			{
				UNITY_SETUP_INSTANCE_ID( IN );
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX( IN );

				float4 color4 = IsGammaSpace() ? float4(0,0,0,1) : float4(0,0,0,1);
				float2 uv_DrawTexture = IN.texcoord.xy * _DrawTexture_ST.xy + _DrawTexture_ST.zw;
				float4 appendResult5 = (float4(color4.rgb , tex2D( _DrawTexture, uv_DrawTexture ).a));
				float2 uv_MainTex = IN.texcoord.xy * _MainTex_ST.xy + _MainTex_ST.zw;
				
				fixed4 c = (( _IsDrawing )?( tex2D( _MainTex, uv_MainTex ) ):( appendResult5 ));
				c.rgb *= c.a;
				return c;
			}
		ENDCG
		}
